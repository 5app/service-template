  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "test": "mocha test/unit/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5app/myservice.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/5app/myservice/issues"
